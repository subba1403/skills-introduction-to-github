{
	"name": "DelimitedText1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Filename": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().Filename,'.csv')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@replace(substring(utcNow(),0,10),'-','/')\n\n",
					"type": "Expression"
				},
				"container": "data"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "DEPARTMENT_ID",
				"type": "String"
			},
			{
				"name": "DEPARTMENT_NAME",
				"type": "String"
			},
			{
				"name": "MANAGER_ID",
				"type": "String"
			},
			{
				"name": "LOCATION_ID",
				"type": "String"
			}
		]
	}
}